
  CREATE OR REPLACE FORCE EDITIONABLE VIEW "SERVICE_COMPANY_DEV"."V_LOCAIS_CONTRATO" ("ID", "BLOQUEADO", "ID_CONTRATO", "CNPJ", "CPF", "NOME", "RAZAO_SOCIAL", "SITUACAO", "TIPO_PESSOA_EMPRESA", "DESCRICAO_LOCAL", "ID_LOCAL_ENDERECO", "ID_GERENCIA", "DESCRICAO_GERENCIA", "DESCRICAO_UNIDADE_CIEE", "ID_UNIDADE_CIEE", "ID_UNIDADE_CIEE_LOCAL", "DESCRICAO_UNIDADE_CIEE_LOCAL", "ID_ENDERECO", "TIPO_LOGRADOURO", "ENDERECO", "NUMERO", "BAIRRO", "COMPLEMENTO", "CEP", "CIDADE", "UF", "ID_CARTEIRA", "DESCRICAO_CARTEIRA", "NOME_ASSISTENTE", "SIGLA_GERENCIA") AS 
  SELECT LOCAIS_CONTRATO.ID,
    LOCAIS_CONTRATO.BLOQUEADO,
    LOCAIS_CONTRATO.ID_CONTRATO,
    LOCAIS_CONTRATO.CNPJ,
    LOCAIS_CONTRATO.CPF,
    LOCAIS_CONTRATO.NOME,
    LOCAIS_CONTRATO.RAZAO_SOCIAL,
    LOCAIS_CONTRATO.SITUACAO,
    LOCAIS_CONTRATO.TIPO_PESSOA_EMPRESA,
	LOCAIS_CONTRATO.DESCRICAO_LOCAL,
    LOCAIS_ENDERECOS.ID AS ID_LOCAL_ENDERECO,
    LOCAIS_ENDERECOS.ID_GERENCIA,
    LOCAIS_ENDERECOS.DESCRICAO_GERENCIA,
    LOCAIS_ENDERECOS.DESCRICAO_UNIDADE_CIEE,
    LOCAIS_ENDERECOS.ID_UNIDADE_CIEE,
	LOCAIS_ENDERECOS.ID_UNIDADE_CIEE_LOCAL,
	LOCAIS_ENDERECOS.DESCRICAO_UNIDADE_CIEE_LOCAL,
    ENDERECOS.ID AS ID_ENDERECO,
    ENDERECOS.TIPO_LOGRADOURO,
    ENDERECOS.ENDERECO,
    ENDERECOS.NUMERO,
    ENDERECOS.BAIRRO,
    ENDERECOS.COMPLEMENTO,
    ENDERECOS.CEP,
    ENDERECOS.CIDADE,
    ENDERECOS.UF,
    LOCAIS_CONTRATO.ID_CARTEIRA,
    RCU.DESCRICAO DESCRICAO_CARTEIRA,
    RPU.NOME NOME_ASSISTENTE,
    GERENCIAS.SIGLA AS SIGLA_GERENCIA
    FROM LOCAIS_CONTRATO
    LEFT JOIN LOCAIS_ENDERECOS ON LOCAIS_CONTRATO.id = LOCAIS_ENDERECOS.ID_LOCAL_CONTRATO
    LEFT JOIN ENDERECOS ON ENDERECOS.ID = LOCAIS_ENDERECOS.ID_ENDERECO
    LEFT JOIN UNIDADES_CIEE ON UNIDADES_CIEE.ID = LOCAIS_ENDERECOS.ID_UNIDADE_CIEE_LOCAL
    LEFT JOIN GERENCIAS ON GERENCIAS.ID = LOCAIS_ENDERECOS.ID_GERENCIA
    LEFT JOIN REP_CARTEIRAS_UNIT RCU ON RCU.ID = LOCAIS_CONTRATO.ID_CARTEIRA
    LEFT JOIN REP_ASSISTENTES_UNIT RAU ON RAU.ID = RCU.ID_ASSISTENTE
    LEFT JOIN REP_PESSOAS_UNIT RPU ON RPU.ID = RAU.ID_PESSOA
    WHERE LOCAIS_CONTRATO.DELETADO = 0;


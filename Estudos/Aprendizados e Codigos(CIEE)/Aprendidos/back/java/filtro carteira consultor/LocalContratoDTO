package br.com.adtsys.ciee.company.contractplaces.dto;
import lombok.AllArgsConstructor;
import lombok.NoArgsConstructor;

import br.com.adtsys.ciee.company.contract.enumeration.CompanyTypeEnum;
import br.com.adtsys.ciee.company.contractplaces.enumeration.ContractPlacesEnum;
import br.com.adtsys.ciee.company.preregistration.enumeration.PurposeTypeEnum;
import lombok.Builder;
import lombok.Data;

@Data
@Builder
@NoArgsConstructor
@AllArgsConstructor
public class LocalContratoDTO {

  private Long id;
  private Long contract;
  private String address;
  private String cep;
  private String managementDescription;
  private String cieeUnitDescription;
  private Long idUnidadeCieeLocal;
  private String descricaoUnidadeCieeLocal;
  private String name;
  private String companyName;
  private String cpf;
  private String cnpj;
  private ContractPlacesEnum status;
  private Boolean blocked;
  private CompanyTypeEnum companyPersonType;
  private PurposeTypeEnum serviceType;
  private String siglaGerencia;
  private String descricaoLocal;
  private Long carteiraConsultor;
  private String descricaoCarteira;
  private String nomeAssistente;
}
